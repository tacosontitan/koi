{
  "manifestVersion": 1,
  "id": "tacosontitan.koi",
  "version": "1.0.0",
  "name": "Koi the Reviewer",
  "publisher": "tacosontitan",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A cheeky AI assistant for reviewing code.",
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "AI",
    "OpenAI",
    "Artificial Intelligence",
    "Code Review",
    "Pull Request"
  ],
  "icons": {
    "default": "resources/textures/logo.png"
  },
  "branding": {
    "color": "rgb(226, 43, 67)",
    "theme": "dark"
  },
  "links": {
    "support": {
      "uri": "https://github.com/tacosontitan/koi/issues",
      "version": "1.0.0"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/tacosontitan/koi/issues"
  },
  "screenshots": [],
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "files": [
    {
      "path": "src"
    }
  ],
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "src"
      }
    }
  ]
}